class Lexer(str code, int p) {

    whitespace() {
        while @p < #len(@code) and @code[@p] == " " {
            @p = @p + 1
        }
    }

    next {
        @whitespace()
        while @p < #len(@code) {
            #print(@code[@p])
            @p = @p + 1
        }
    }

}

def lexer = Lexer("          class hello", 0)
lexer.next


